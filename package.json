{
  "name": "google-drive-image-upload",
  "version": "1.0.0",
  "description": "A Node.js server for uploading images directly to Google Drive with user-specific folder organization",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "echo 'Web app ready for Capacitor'",
    "cap:init": "cap init",
    "cap:add:android": "cap add android",
    "cap:add:ios": "cap add ios",
    "cap:sync": "cap sync",
    "cap:open:android": "cap open android",
    "cap:open:ios": "cap open ios",
    "cap:run:android": "cap run android",
    "cap:run:ios": "cap run ios",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "google-drive",
    "image-upload",
    "nodejs",
    "express",
    "file-upload",
    "cloud-storage",
    "oauth2"
  ],
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Danish-Razzaq/drive-image-uploader.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/google-drive-image-upload/issues"
  },
  "homepage": "https://github.com/yourusername/google-drive-image-upload#readme",
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "dependencies": {
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "googleapis": "^128.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "path": "^0.12.7",
    "fs-extra": "^11.1.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "typescript": "^5.0.0",
    "@capacitor/core": "^5.5.1",
    "@capacitor/cli": "^5.5.1",
    "@capacitor/android": "^5.5.1",
    "@capacitor/ios": "^5.5.1"
  }
}